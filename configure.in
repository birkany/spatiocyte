AC_REVISION([$Id$])
dnl
AC_INIT(autogen.sh)
dnl AC_CONFIG_AUX_DIR()
dnl
dnl 
dnl 
AC_CANONICAL_SYSTEM
AM_INIT_AUTOMAKE(package, version, do-not-define)
AC_PROG_MAKE_SET
dnl
dnl
dnl assume korandom and dmtool packages are there
ecell_have_korandom=true
ecell_have_dmtool=true
dnl
AC_MSG_CHECKING([for packages to be configured and built])
ecell_subdirs="ecell"
test "$ecell_have_korandom" != true && ecell_subdirs="korandom $ecell_subdirs"
test "$ecell_have_dmtool" != true && ecell_subdirs="dmtool $ecell_subdirs"
AC_MSG_RESULT($ecell_subdirs)
AC_CONFIG_SUBDIRS($ecell_subdirs)
dnl
dnl
dnl Create the top-level Makefile.
dnl
