#!/usr/bin/env ecell3


import sys

import ecell.ecs
import ecell.Session


#FIXME: commandline processing needed

anEssFileName = None

if len( sys.argv ) > 1:
	anEssFileName = sys.argv[1]

#FIXME:
aParameters = {}


aSimulator = ecell.ecs.Simulator()
aSession = ecell.Session.Session( aSimulator )

if anEssFileName:
	aSession.loadScript( anEssFileName, aParameters )
else:
	aSession.interact( aParameters )

