<?xml version="1.0"?>
<GTK-Interface>

<project>
  <name>Progressbar</name>
  <program_name>progressbar</program_name>
  <directory></directory>
  <source_directory>src</source_directory>
  <pixmaps_directory>pixmaps</pixmaps_directory>
  <language>C</language>
  <gnome_support>True</gnome_support>
  <gettext_support>True</gettext_support>
</project>

<widget>
  <class>GtkWindow</class>
  <name>window1</name>
  <width>300</width>
  <height>30</height>
  <title>window1</title>
  <type>GTK_WINDOW_TOPLEVEL</type>
  <position>GTK_WIN_POS_NONE</position>
  <modal>False</modal>
  <allow_shrink>False</allow_shrink>
  <allow_grow>False</allow_grow>
  <auto_shrink>False</auto_shrink>

  <widget>
    <class>GtkHBox</class>
    <name>hbox1</name>
    <width>292</width>
    <height>30</height>
    <homogeneous>False</homogeneous>
    <spacing>0</spacing>

    <widget>
      <class>GtkFrame</class>
      <name>frame2</name>
      <label_xalign>0</label_xalign>
      <shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
      <child>
	<padding>0</padding>
	<expand>False</expand>
	<fill>False</fill>
      </child>

      <widget>
	<class>GtkLabel</class>
	<name>label1</name>
	<width>120</width>
	<label>label1</label>
	<justify>GTK_JUSTIFY_CENTER</justify>
	<wrap>False</wrap>
	<xalign>0.5</xalign>
	<yalign>0.5</yalign>
	<xpad>0</xpad>
	<ypad>0</ypad>
      </widget>
    </widget>

    <widget>
      <class>GtkFrame</class>
      <name>frame1</name>
      <border_width>1</border_width>
      <width>105</width>
      <height>30</height>
      <label_xalign>0</label_xalign>
      <shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
      <child>
	<padding>0</padding>
	<expand>False</expand>
	<fill>False</fill>
      </child>

      <widget>
	<class>GtkProgressBar</class>
	<name>progressbar1</name>
	<width>100</width>
	<height>30</height>
	<value>0</value>
	<lower>0</lower>
	<upper>100</upper>
	<bar_style>GTK_PROGRESS_CONTINUOUS</bar_style>
	<orientation>GTK_PROGRESS_LEFT_TO_RIGHT</orientation>
	<activity_mode>False</activity_mode>
	<show_text>False</show_text>
	<format>%P %%</format>
	<text_xalign>0.5</text_xalign>
	<text_yalign>0.5</text_yalign>
      </widget>
    </widget>

    <widget>
      <class>GtkSpinButton</class>
      <name>spinbutton1</name>
      <width>50</width>
      <can_focus>True</can_focus>
      <signal>
	<name>activate</name>
	<handler>input1</handler>
	<last_modification_time>Mon, 13 Aug 2001 08:24:25 GMT</last_modification_time>
      </signal>
      <signal>
	<name>changed</name>
	<handler>input2</handler>
	<last_modification_time>Mon, 13 Aug 2001 08:28:52 GMT</last_modification_time>
      </signal>
      <climb_rate>1</climb_rate>
      <digits>0</digits>
      <numeric>True</numeric>
      <update_policy>GTK_UPDATE_ALWAYS</update_policy>
      <snap>False</snap>
      <wrap>False</wrap>
      <value>-100</value>
      <lower>-100</lower>
      <upper>100</upper>
      <step>1</step>
      <page>10</page>
      <page_size>10</page_size>
      <child>
	<padding>0</padding>
	<expand>False</expand>
	<fill>False</fill>
      </child>
    </widget>

    <widget>
      <class>GtkCheckButton</class>
      <name>checkbutton1</name>
      <can_focus>True</can_focus>
      <signal>
	<name>button_press_event</name>
	<handler>button_press_event</handler>
	<last_modification_time>Fri, 10 Aug 2001 07:23:23 GMT</last_modification_time>
      </signal>
      <label></label>
      <active>False</active>
      <draw_indicator>True</draw_indicator>
      <child>
	<padding>0</padding>
	<expand>False</expand>
	<fill>False</fill>
      </child>
    </widget>
  </widget>
</widget>

</GTK-Interface>
